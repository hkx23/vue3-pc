    "options": {
      "workshopId": {
        "label": "车间",
        "comp": "bcmp-select-business",
        "bind": {
          "type": "workshop",
          "showTitle": false
        },
        "defaultVal": "",
        "operator": "eq"
      },
      "workcenterId": {
        "label": "工作中心",
        "comp": "bcmp-select-business",
