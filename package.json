{
  "name": "mes-desktop",
  "version": "1.0.0",
  "scripts": {
    "dev:mock": "vite --mode mock",
    "dev": "vite --mode development",
    "build": "cross-env LIMIT=8192 increase-memory-limit && vue-tsc --noEmit && vite build --mode production",
    "build:dev": "cross-env LIMIT=8192 increase-memory-limit && vue-tsc --noEmit && vite build --mode development",
    "build:uat": "cross-env LIMIT=8192 increase-memory-limit && vue-tsc --noEmit && vite build --mode uat",
    "build:poc": "cross-env LIMIT=8192 increase-memory-limit && vue-tsc --noEmit && vite build --mode poc",
    "build:changqingdev": "cross-env LIMIT=8192 increase-memory-limit && vue-tsc --noEmit && vite build --mode changqingdev",
    "tsc": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint --ext .vue,.js,.jsx,.ts,.tsx src/ --max-warnings 0",
    "lint:fix": "eslint --ext .vue,.js,jsx,.ts,.tsx src/ --max-warnings 0 --fix",
    "stylelint": "stylelint src/**/*.{html,vue,sass,less}",
    "stylelint:fix": "stylelint --fix src/**/*.{html,vue,css,sass,less}",
    "prepare": "husky install",
    "fix-memory-limit": "cross-env LIMIT=8192 increase-memory-limit"
  },
